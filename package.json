    "expo": "~49.0.7",
    "expo-dev-client": "~2.4.11",
    "expo-location": "~16.1.0",
    "expo-status-bar": "~1.6.0",
    "formik": "^2.4.3",
    "goog-places-api": "^0.0.3",
    "nativewind": "^2.0.11",
    "prettier": "^3.0.1",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-dotenv": "^3.4.9",
