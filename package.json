    "Tailwind",
    "NativeWind",
    "Typescript",
    "YupJs",
   "ReactNavigation",
   "Heroicons"

  ],
  "author": {
    "name": "Jean Carlos Piguave Alvarado",
    "email": "piguavejc@gmail.com",
